import { Button, VerticalBox} from "std-widgets.slint";
import { Router,CurrentPage } from "./global.slint";


export component Home inherits VerticalBox {
    for name in Router.pages_names : Button {
        text: name;
        clicked => {
            Router.prepare-sheet-page(name);
            Router.page = CurrentPage.SheetHome
        }
    }
}
